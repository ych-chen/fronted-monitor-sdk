{
  "name": "frontend-monitor-sdk",
  "version": "1.0.0",
  "description": "A frontend monitoring SDK based on OpenTelemetry",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.js",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config=jest.integration.config.js",
    "test:e2e": "playwright test",
    "dev:quick-test": "node tools/quick-test.js",
    "dev:server": "ts-node tools/dev-server.ts",
    "dev:link": "npm run build && npm link",
    "dev:unlink": "npm unlink -g",
    "dev:pack": "npm run build && npm pack",
    "dev:yalc": "npm run build && yalc publish",
    "dev:yalc:watch": "concurrently \"npm run dev\" \"npm run dev:yalc\"",
    "dev:yalc:clean": "yalc clean",
    "test:yalc": "node scripts/test-yalc-workflow.js",
    "playground": "npm run dev:server",
    "debug": "npm run build && npm run dev:quick-test"
  },
  "keywords": [
    "opentelemetry",
    "monitoring",
    "frontend",
    "performance",
    "tracing",
    "metrics"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/frontend-monitor-sdk.git"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-web": "^0.38.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.48.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.48.0",
    "@opentelemetry/resources": "^1.18.1",
    "@opentelemetry/sdk-metrics": "^1.18.1",
    "@opentelemetry/sdk-trace-web": "^1.18.1",
    "@opentelemetry/semantic-conventions": "^1.18.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "playwright": "^1.40.0",
    "prettier": "^3.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "ts-jest": "^29.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "engines": {
    "node": ">=14.0.0 <=22.0.0"
  }
}
